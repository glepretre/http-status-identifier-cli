{
  "name": "http-status-identifier-cli",
  "version": "1.0.2",
  "description": "Identify http status",
  "bin": {
    "hs": "build/executables/hs.js"
  },
  "directories": {
    "lib": "build",
    "test": "test"
  },
  "scripts": {
    "compile": "./node_modules/.bin/babel --presets es2015,stage-0 -d build/ src/",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha test --  --recursive --compilers js:babel-core/register",
    "coveralls": "node_modules/.bin/coveralls < coverage/lcov.info",
    "prepublish": "npm run compile",
    "lint": "eslint --ext .js .",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "release": {
    "branch": "master",
    "analyzeCommits": "@semantic-release/commit-analyzer",
    "generateNotes": "@semantic-release/release-notes-generator"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/http-status-identifier-cli.git"
  },
  "keywords": [
    "http"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaebradley/http-status-identifier-cli/issues"
  },
  "homepage": "https://github.com/jaebradley/http-status-identifier-cli#readme",
  "dependencies": {
    "cli-table2": "^0.2.0",
    "commander": "^2.9.0",
    "http-status-identifier": "^1.0.0",
    "node-emoji": "^1.5.1",
    "open": "0.0.5"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^4.0.0",
    "@semantic-release/release-notes-generator": "^5.0.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-istanbul": "^0.12.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-immutable": "^1.6.0",
    "conventional-changelog-eslint": "^0.2.0",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.9.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "semantic-release": "^8.2.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0"
  }
}
